        .btn-sm {
            padding: 12px 16px;
            min-width: 44px;
            min-height: 44px;
            font-size: 0.75rem;
            display: inline-flex;
            align-items: center;
            justify-content: center;
        }

        .btn-sm .icon {
            width: 12px;
            height: 12px;
            margin-right: 4px;
        }

        .empty-state {
            text-align: center;
            padding: 60px 20px;
